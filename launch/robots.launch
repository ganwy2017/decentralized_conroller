<?xml version="1.0"?>
<launch> 
    <!-- BEGIN ROBOT 1-->   
    <group ns="robot1">
        <param name="tf_prefix" value="robot1_tf" />
    </group>
    <include file="$(find formation_control_simu)/launch/one_robot.launch" >
        <arg name="init_pose_x" value="2" />
        <arg name="init_pose_y" value="2"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot1" />
    </include>

    <!-- BEGIN ROBOT 2 -->   
    <group ns="robot2">
        <param name="tf_prefix" value="robot2_tf" />
    </group>
    <include file="$(find formation_control_simu)/launch/one_robot.launch" >
        <arg name="init_pose_x" value="4" />
        <arg name="init_pose_y" value="-4"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot2" />
    </include>

    <!-- BEGIN ROBOT 3 -->   
    <group ns="robot3">
        <param name="tf_prefix" value="robot3_tf" />
    </group>
    <include file="$(find formation_control_simu)/launch/one_robot.launch" >
        <arg name="init_pose_x" value="6" />
        <arg name="init_pose_y" value="-4"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot3" />
    </include>

    <!-- BEGIN ROBOT 4 -->   
    <group ns="robot4">
        <param name="tf_prefix" value="robot4_tf" />
    </group>
    <include file="$(find formation_control_simu)/launch/one_robot.launch" >
        <arg name="init_pose_x" value="-6" />
        <arg name="init_pose_y" value="4"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot4" />
    </include>

    <!-- BEGIN ROBOT 5 -->   
    <group ns="robot5">
        <param name="tf_prefix" value="robot5_tf" />
    </group>
    <include file="$(find formation_control_simu)/launch/one_robot.launch" >
        <arg name="init_pose_x" value="6" />
        <arg name="init_pose_y" value="4"/>
        <arg name="init_pose_a" value="3.14"/>
        <arg name="robot_name"  value="robot5" />
    </include>
</launch>
